{
  "name": "game-hub",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build --turbopack",
    "start": "next start",
    "lint": "biome check",
    "format": "biome format --write",
    "fix": "biome check --fix",
    "genkit:dev": "genkit start -- tsx ai/dev.ts",
    "genkit:watch": "genkit start -- tsx --watch ai/dev.ts",
    "algobuild": "algokit compile ts smart_contracts --output-source-map --out-dir artifacts && algokit generate client smart_contracts/artifacts --output {app_spec_dir}/{contract_name}Client.ts",
    "deploy:algo": "tsx smart_contracts/index.ts",
    "deploy:ci": "tsx -r dotenv/config smart_contracts/index.ts",
    "generate:clients": "for d in smart_contracts/artifacts/*; do if [ -d \"$d\" ]; then cname=$(basename \"$d\"); algokit generate client $d --output contracts/${cname}.ts --language typescript; fi; done",
    "generate": "pnpm algobuild && pnpm generate:clients",
    "check-types": "tsc --noEmit"
  },
  "dependencies": {
    "@agoralabs-sh/avm-web-provider": "^1.7.0",
    "@ai-sdk/google": "^2.0.12",
    "@ai-sdk/openai": "^2.0.24",
